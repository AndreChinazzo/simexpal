instances:
  - repo: local
    items:
      - random_500.list
      - partially_sorted_500.list

instdir: "<path_to_instances_directory>"

experiments:
  - name: insertion-sort
    args: ['./demo.py', '--algo=insertion_sort', '@INSTANCE@']
    output: stdout
  - name: bubble-sort
    args: ['./demo.py', '--algo=bubble_sort', '@INSTANCE@']
    output: stdout
